    %set/v v0x8d50e28_0, 8, 2;
    %set/v v0x8d50d00_0, 0, 1;
    %jmp T_0.8;
T_0.7 ;
    %set/v v0x8d50e28_0, 0, 2;
    %set/v v0x8d50d00_0, 0, 1;
T_0.8 ;
T_0.6 ;
    %jmp T_0.4;
T_0.1 ;
    %load/v 8, v0x8d50dc0_0, 1;
    %jmp/0xz  T_0.9, 8;
    %movi 8, 2, 2;
    %set/v v0x8d50e28_0, 8, 2;
    %set/v v0x8d50d00_0, 0, 1;
    %jmp T_0.10;
T_0.9 ;
    %load/v 8, v0x8d50d60_0, 1;
    %jmp/0xz  T_0.11, 8;
    %set/v v0x8d50e28_0, 1, 2;
    %set/v v0x8d50d00_0, 0, 1;
    %jmp T_0.12;
T_0.11 ;
    %movi 8, 1, 2;
    %set/v v0x8d50e28_0, 8, 2;
    %set/v v0x8d50d00_0, 0, 1;
T_0.12 ;
T_0.10 ;
    %jmp T_0.4;
T_0.2 ;
    %load/v 8, v0x8d50dc0_0, 1;
    %jmp/0xz  T_0.13, 8;
    %set/v v0x8d50e28_0, 1, 2;
