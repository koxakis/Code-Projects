    %jmp T_0.14;
T_0.13 ;
    %load/v 8, v0x8d50d60_0, 1;
    %jmp/0xz  T_0.15, 8;
    %set/v v0x8d50e28_0, 0, 2;
    %set/v v0x8d50d00_0, 1, 1;
    %jmp T_0.16;
T_0.15 ;
    %movi 8, 2, 2;
    %set/v v0x8d50e28_0, 8, 2;
    %set/v v0x8d50d00_0, 0, 1;
T_0.16 ;
T_0.14 ;
    %jmp T_0.4;
T_0.3 ;
    %load/v 8, v0x8d50dc0_0, 1;
    %load/v 9, v0x8d50d60_0, 1;
    %or 8, 9, 1;
    %jmp/0xz  T_0.17, 8;
    %set/v v0x8d50e28_0, 0, 2;
    %set/v v0x8d50d00_0, 1, 1;
    %jmp T_0.18;
T_0.17 ;
    %set/v v0x8d50e28_0, 1, 2;
    %set/v v0x8d50d00_0, 0, 1;
T_0.18 ;
    %jmp T_0.4;
T_0.4 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x8d30e20;
T_1 ;
    %wait E_0x8d51860;
    %load/v 8, v0x8d50e88_0, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 2, 0;
    %assign/v0 v0x8d50f08_0, 0, 0;
