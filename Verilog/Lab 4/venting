    %ix/load 0, 1, 0;
    %assign/v0 v0x8d51138_0, 0, 0;
    %delay 500, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x8d51058_0, 0, 1;
    %delay 500, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0x8d51058_0, 0, 0;
