#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x86d9200 .scope module, "test_8_16" "test_8_16" 2 2;
 .timescale -9 -11;
v0x86d5e08_0 .var "is_nice", 0 0;
v0x86d5e88_0 .var "sexy_time", 0 0;
v0x86d5ef8_0 .net "signed_borat", 3 0, v0x86d5d40_0; 1 drivers
v0x86d5f68_0 .var "unsigned_borat", 3 0;
S_0x86b5e38 .scope module, "c1" "test" 2 22, 3 1, S_0x86d9200;
 .timescale 0 0;
v0x86b5eb8_0 .net "is_nice", 0 0, v0x86d5e08_0; 1 drivers
v0x86d5c80_0 .net "sexy_time", 0 0, v0x86d5e88_0; 1 drivers
v0x86d5ce0_0 .alias "signed_borat", 3 0, v0x86d5ef8_0;
v0x86d5d40_0 .var "signed_tmp", 3 0;
v0x86d5da8_0 .net "unsigned_borat", 3 0, v0x86d5f68_0; 1 drivers
E_0x86d8698 .event posedge, v0x86d5c80_0;
    .scope S_0x86b5e38;
T_0 ;
    %wait E_0x86d8698;
    %load/v 8, v0x86b5eb8_0, 1;
    %jmp/0xz  T_0.0, 8;
    %load/v 8, v0x86d5da8_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x86d5d40_0, 0, 8;
    %ix/load 0, 1, 0;
    %ix/load 1, 3, 0;
    %assign/v0/x1 v0x86d5d40_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x86d5da8_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x86d5d40_0, 0, 8;
    %ix/load 0, 1, 0;
    %ix/load 1, 3, 0;
    %assign/v0/x1 v0x86d5d40_0, 0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x86d9200;
T_1 ;
    %set/v v0x86d5e08_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x86d9200;
T_2 ;
    %set/v v0x86d5f68_0, 0, 4;
    %end;
    .thread T_2;
    .scope S_0x86d9200;
T_3 ;
    %set/v v0x86d5e88_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x86d9200;
T_4 ;
    %delay 100, 0;
    %load/v 8, v0x86d5e88_0, 1;
    %inv 8, 1;
    %set/v v0x86d5e88_0, 8, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x86d9200;
T_5 ;
    %vpi_call 2 12 "$dumpfile", "8_16_test.vcd";
    %vpi_call 2 13 "$dumpvars", 1'sb0, S_0x86d9200;
    %delay 1000, 0;
    %set/v v0x86d5e08_0, 1, 1;
    %delay 2000, 0;
    %set/v v0x86d5f68_0, 1, 4;
    %delay 50000, 0;
    %vpi_call 2 18 "$finish";
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "unsiged_singed_test.v";
    "test.v";
