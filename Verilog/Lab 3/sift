#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x924d0b0 .scope module, "Shifter_test" "Shifter_test" 2 2;
 .timescale -9 -11;
v0x924a038_0 .var "clk", 0 0;
v0x924a0b8_0 .var "in", 7 0;
v0x924a128_0 .var "left_in", 0 0;
v0x924a198_0 .net "out", 7 0, v0x9249fd8_0; 1 drivers
S_0x922a048 .scope module, "c1" "shifter_8bit" 2 31, 3 1, S_0x924d0b0;
 .timescale 0 0;
v0x922a0c8_0 .net "clk", 0 0, v0x924a038_0; 1 drivers
v0x9249eb0_0 .net "in", 7 0, v0x924a0b8_0; 1 drivers
v0x9249f10_0 .net "left_in", 0 0, v0x924a128_0; 1 drivers
v0x9249f70_0 .alias "out", 7 0, v0x924a198_0;
v0x9249fd8_0 .var "tmp", 7 0;
E_0x924c588 .event posedge, v0x922a0c8_0;
    .scope S_0x922a048;
T_0 ;
    %wait E_0x924c588;
    %load/v 8, v0x9249f10_0, 1;
    %jmp/0xz  T_0.0, 8;
    %load/v 8, v0x9249eb0_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftl/i0  8, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x9249fd8_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x9249eb0_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftr/i0  8, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x9249fd8_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x924d0b0;
T_1 ;
    %set/v v0x924a038_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x924d0b0;
T_2 ;
    %set/v v0x924a128_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x924d0b0;
T_3 ;
    %movi 8, 24, 8;
    %set/v v0x924a0b8_0, 8, 8;
    %end;
    .thread T_3;
    .scope S_0x924d0b0;
T_4 ;
    %delay 100, 0;
    %load/v 8, v0x924a038_0, 1;
    %inv 8, 1;
    %set/v v0x924a038_0, 8, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x924d0b0;
T_5 ;
    %vpi_call 2 15 "$dumpfile", "Shifter_test.vcd";
    %vpi_call 2 16 "$dumpvars", 1'sb0, S_0x924d0b0;
    %delay 500, 0;
    %set/v v0x924a128_0, 1, 1;
    %delay 500, 0;
    %set/v v0x924a128_0, 0, 1;
    %delay 500, 0;
    %movi 8, 231, 8;
    %set/v v0x924a0b8_0, 8, 8;
    %delay 500, 0;
    %set/v v0x924a128_0, 1, 1;
    %delay 500, 0;
    %set/v v0x924a128_0, 0, 1;
    %delay 50000, 0;
    %vpi_call 2 28 "$finish";
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "sifter_test.v";
    "8bit_slider.v";
